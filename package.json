{
  "name": "azure-devops-bpmn-tooling",
  "version": "1.0.0",
  "description": "Azure DevOps extension facilitating work with BPMN models",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && npm run test && webpack --mode production",
    "compile:dev": "npm run clean && npm run test && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --rev-version",
    "postbuild:dev": "npm run package-extension:dev -- --rev-version",
    "package-extension": "tfx extension create --manifest-globs vss-extension.json src/**/*.json",
    "package-extension:dev": "tfx extension create --manifest-globs vss-extension.json src/**/*.json  --overrides-file configs/dev.json",
    "publish-extension": "tfx extension publish --manifest-globs vss-extension.json src/**/*.json",
    "test": "cross-env TEST_REPORT_FILENAME=test-results.xml jest --verbose"
  },
  "author": "Grzegorz Sobczak",
  "license": "MIT",
  "dependencies": {
    "azure-devops-extension-api": "^4.231.0",
    "azure-devops-extension-sdk": "^4.0.2",
    "azure-devops-ui": "^2.167.87",
    "bpmn-js": "^16.5.0",
    "bpmn-js-differ": "^2.0.2",
    "bpmn-moddle": "^8.1.0",
    "lodash": "^4.17.21",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-json-tree": "^0.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.24.5",
    "@glen/jest-raw-loader": "^2.0.0",
    "@testing-library/jest-dom": "^6.4.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-preset-react-app": "^10.0.1",
    "base64-inline-loader": "^2.0.1",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-modules-typescript-loader": "^4.0.1",
    "jest": "^29.7.0",
    "jest-junit-reporter": "^1.1.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.5",
    "sass": "^1.70.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tfx-cli": "^0.16.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
